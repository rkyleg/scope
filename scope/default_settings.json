{
"save_workspace_on_close":1,
"widgetstyle":"plastique",
"style":"style/default.css",
"showPath":1,
"visibleLineTracking":1,
"checkFileChanges":2,

"activePlugins":[
    "filebrowser",
    "outline",
    "py_console",
    "find_replace",
    "output",
    "snippets",
    "preview",
    "scratch",
    "home",
    "find_files",
    "console_launcher",
    "webbrowser"
],

"activeEditors":["ace","scintilla","ckeditor"],

"window":{
    "openMode":1,
    "size":{
        "width":"100%",
        "height":"100%"
    },

    "margin": {
        "left":50,
        "right":50,
        "top":50,
        "bottom":50
    }
},
"window_switcher":{
    "height":126,
    "view_folder":0
},

"pluginLeft":{
    "width":260,
    "visible":1,
    "tabPosition":"top",
    "showTabText":1
},
"pluginBottom": {
    "visible":1,
    "height":180,
    "showTabText":1
},
"pluginRight": {
    "visible":0,
    "width":"50%",
    "tabPosition":"bottom",
    "showTabText":1,
    "leftToggle":1
},

"editors":{
    "ace": {
        "wordwrap":0,
        "theme":"twilight",
        "behavioursEnabled":1,
        "wrapBehavioursEnabled":1,
        "newLineMode":"auto",
        "showWhitespace":0
    },
    "scintilla": {
        "wordwrap":0,
        "autocomplete":1,
        "newLineMode":"auto",
        "showWhitespace":0,
        "theme":"dark"
    },
    "ckeditor":{
    }
},

"prog_lang": {
    "default": {
        "editor":"scintilla",
        "previewLocation":"plugin"
    },
    "python": {
        "editor":"scintilla",
        "run":"python -u"
    },
    "html": {
        "editor":"ace",
        "run":"preview"
    },
    "css": {
        "editor":"ace"
    },
    "text": {
        "editor":"scintilla"
    },
    "javascript": {
        "editor":"ace"
    },
    "markdown": {
        "editor":"ace",
        "wordwrap":1,
        "run":"preview",
        "preview_cmd":"markdown"
    },
    "json": {
        "editor":"ace"
    }
},

"extensions": {
    "py":"python",
    "pyw":"python",
    "js":"javascript",
    "htm":"html",
    "css":"css",
    "style":"css",
    "xml":"xml",
    "ui":"xml",
    "qml":"javascript",
    "sh":"bash",
    "txt":"text",
    "csv":"text",
    "bat":"batch",
    "rb":"ruby",
    "md":"markdown",
    "c":"cpp",
    "h":"cpp",
    "ino":"cpp",
    "conf":"ini"
},

"plugins": {
    "outline": {
        "alwaysUpdate":0
    },
    "filebrowser": {
        "showAll":0,
        "defaultPath":""
    },
    "snippets": {
        "path":""
    },
    "find_files": {
        "default_ext":".py"
    }
},

"network": {
    "proxy":""
}

}